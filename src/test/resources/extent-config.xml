<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>

        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- enables timeline -->
        <!-- defaults to true -->
        <enableTimeline>true</enableTimeline>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>My Spark Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Build Test Report</reportName>
    </configuration>
</extentreports>